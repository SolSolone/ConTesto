    <bpmn:task id="Task_CalcolaManodopera" name="Calcola manodopera" />
    <bpmn:task id="Task_AnalisiCosti" name="Analisi costi/prezzi (Edison)" />
    <bpmn:exclusiveGateway id="Gateway_MargineOK" name="Margine OK?" />
    <bpmn:task id="Task_AggiustaSconti" name="Aggiusta sconti/ricarichi" />
    <bpmn:task id="Task_PersonalizzaStampa" name="Personalizza stampa" />
    <bpmn:task id="Task_GeneraPDF" name="Genera PDF" />

    <!-- Task Titolare -->
    <bpmn:task id="Task_Revisione" name="Revisione e approvazione" />
    <bpmn:exclusiveGateway id="Gateway_Approva" name="Approva?" />
    <bpmn:task id="Task_ConvertiCommessa" name="Converti in commessa Edison" />
    <bpmn:task id="Task_GeneraOrdiniFornitore" name="Genera ordini fornitore" />

