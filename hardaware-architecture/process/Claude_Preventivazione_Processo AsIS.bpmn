                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_GestionePreventivi" 
                  targetNamespace="http://pmi.esempio.it/preventivi">

  <bpmn:collaboration id="Collaboration_Preventivi">
    <bpmn:participant id="Participant_Cliente" name="CLIENTE" processRef="Process_Cliente" />
    <bpmn:participant id="Participant_PMI" name="PMI (15 pers, Edison)" processRef="Process_PMI" />
    
    <bpmn:messageFlow id="Flow_RichiestaPreventivo" sourceRef="Task_Cliente_RichiedePreventivo" targetRef="Event_Start_RichiestaArrivata" />
    <bpmn:messageFlow id="Flow_InvioOfferta" sourceRef="Task_InviaOffertaEmail" targetRef="Task_Cliente_RiceveOfferta" />
    <bpmn:messageFlow id="Flow_Conferma" sourceRef="Task_Cliente_Conferma" targetRef="Event_ConfermaOrdine" />
  </bpmn:collaboration>

  <!-- PROCESSO CLIENTE -->
  <bpmn:process id="Process_Cliente" name="Cliente" isExecutable="false">
    <bpmn:startEvent id="Start_Cliente" name="Necessità lavoro" />
    <bpmn:task id="Task_Cliente_RichiedePreventivo" name="Richiede preventivo" />
    <bpmn:task id="Task_Cliente_RiceveOfferta" name="Riceve offerta" />
    <bpmn:exclusiveGateway id="Gateway_Cliente_Decide" name="Valuta?" />
    <bpmn:task id="Task_Cliente_Conferma" name="Conferma ordine" />
    <bpmn:endEvent id="End_Cliente_OK" name="Confermato" />
    <bpmn:endEvent id="End_Cliente_KO" name="Perso" />
    
    <bpmn:sequenceFlow id="Flow_C1" sourceRef="Start_Cliente" targetRef="Task_Cliente_RichiedePreventivo" />
    <bpmn:sequenceFlow id="Flow_C2" sourceRef="Task_Cliente_RichiedePreventivo" targetRef="Task_Cliente_RiceveOfferta" />
    <bpmn:sequenceFlow id="Flow_C3" sourceRef="Task_Cliente_RiceveOfferta" targetRef="Gateway_Cliente_Decide" />
    <bpmn:sequenceFlow id="Flow_C4" sourceRef="Gateway_Cliente_Decide" targetRef="Task_Cliente_Conferma" name="OK" />
    <bpmn:sequenceFlow id="Flow_C5" sourceRef="Gateway_Cliente_Decide" targetRef="End_Cliente_KO" name="NO" />
    <bpmn:sequenceFlow id="Flow_C6" sourceRef="Task_Cliente_Conferma" targetRef="End_Cliente_OK" />
  </bpmn:process>

  <!-- PROCESSO PMI -->
  <bpmn:process id="Process_PMI" name="PMI" isExecutable="true">
    <bpmn:laneSet id="LaneSet_PMI">
      <bpmn:lane id="Lane_Amministrazione" name="AMMINISTRAZIONE">
        <bpmn:flowNodeRef>Event_Start_RichiestaArrivata</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RegistraRichiesta</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AssegnaResponsabile</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_ClienteNuovo</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RecuperaDatiStorici</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_InviaOffertaEmail</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ArchiviaPreventivo</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_OffertaInviata</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_Responsabile" name="RESPONSABILE TECNICO">
        <bpmn:flowNodeRef>Task_Sopralluogo</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_DefinizioneCapitolato</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RicercaMateriali</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CompilaPreventivo</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CalcolaManodopera</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AnalisiCosti</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_MargineOK</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AggiustaSconti</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_PersonalizzaStampa</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_GeneraPDF</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_Titolare" name="TITOLARE">
        <bpmn:flowNodeRef>Task_Revisione</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Approva</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Event_ConfermaOrdine</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ConvertiCommessa</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_GeneraOrdiniFornitore</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_Vinto</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <!-- Eventi -->
    <bpmn:startEvent id="Event_Start_RichiestaArrivata" name="Richiesta arrivata" />
    <bpmn:intermediateCatchEvent id="Event_ConfermaOrdine" name="Conferma ordine" />
