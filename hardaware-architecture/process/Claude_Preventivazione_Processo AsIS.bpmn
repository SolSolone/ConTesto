    <bpmn:sequenceFlow id="Flow_4" sourceRef="Gateway_ClienteNuovo" targetRef="Task_RecuperaDatiStorici" name="Esistente" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Gateway_ClienteNuovo" targetRef="Task_Sopralluogo" name="Nuovo" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_RecuperaDatiStorici" targetRef="Task_Sopralluogo" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Task_Sopralluogo" targetRef="Task_DefinizioneCapitolato" />
    <bpmn:sequenceFlow id="Flow_8" sourceRef="Task_DefinizioneCapitolato" targetRef="Task_RicercaMateriali" />
    <bpmn:sequenceFlow id="Flow_9" sourceRef="Task_RicercaMateriali" targetRef="Task_CompilaPreventivo" />
    <bpmn:sequenceFlow id="Flow_10" sourceRef="Task_CompilaPreventivo" targetRef="Task_CalcolaManodopera" />
    <bpmn:sequenceFlow id="Flow_11" sourceRef="Task_CalcolaManodopera" targetRef="Task_AnalisiCosti" />
